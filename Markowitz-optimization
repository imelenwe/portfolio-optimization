read_csv(path)\n    # pick only what we need\n    df['Symbol'] = t\n    df = df[['Date', 'Symbol', 'Close']].dropna()\n    parts.append(df)\n\n\ndf_all = pd.concat(parts, ignore_index=True)\nsymbols = sorted(df_all['Symbol'].unique())\nwide = df_all.pivot_table(index='Date', columns='Symbol', values='Close').sort_index()\nwide.index = pd.to_datetime(wide.index)\nwide = wide[symbols]   # enforce order\nwide = wide.dropna(how='any')\n\n#print(wide.head())\n#print(wide.tail())\n\n\n# resample and calculate mean and cov\n\nstart = wide.resample('YE').first()\nend = wide.resample('YE').last()\nannual_returns = (end/start) - 1\nannual_returns = annual_returns.dropna(how='any')  \n\n#print(annual_returns.shape)\nprint(annual_returns.head())\n\nmu = annual_returns.mean()\ncov = annual_returns.cov()\n\nprint(\"mu:\\n\", mu)\nprint(\"Cov:\\n\", cov)\nsymbols = list(mu.index)\nN = len(symbols)\n\nmu_vals = mu.values\ncov_vals = cov.values\n\ndef solve_markowitz_optimization(mu_vals, cov_vals, lam=1.0):\n    w = cp.Variable(N)\n    objective = cp.quad_form(w, cov_vals) - lam*(mu_vals @ w)\n    constraint = [cp.sum(w) == 1, w >= 0]\n    problem = cp.Problem(cp.Minimize(objective), constraint)\n    problem.solve(solver=cp.OSQP, verbose=False)\n    w = np.array(w.value).flatten()\n    w = np.clip(w, 0, None)\n    w = w/w.sum()\n    return w\n\n\n# sweep lambda\nlams = np.linspace(0, 1, 40) # generate 40 lambda values between 0 to 10\nrisks = []\nrets  = []\nweights = []\n\nfor lam in lams:\n    w = solve_markowitz_optimization(mu_vals,cov_vals,lam)\n    weights.append(w)\n    rets.append(mu_vals @ w)\n    risks.append(np.sqrt(w @ cov_vals @ w))\n\n# plot frontier\nplt.figure(figsize=(6,4))\nplt.plot(risks, rets, '-o', markersize=4)\n\nfor i, lam in enumerate(lams):\n    plt.text(risks[i], rets[i], f\"{lam:.2f}\", fontsize=6)\n    \nplt.xlabel(\"Risk\")\nplt.ylabel(\"Return\")\nplt.title(\"Efficient Frontier\")\nplt.grid(True)\nplt.show()\n\n\n# show one portfolio\nidx = 20  # pick a mid frontier point\nprint(\"lambda =\", lams[idx])\nprint(pd.Series(weights[idx], index=symbols).round(4))\nprint(\"Return:\", rets[idx])\nprint(\"Risk:\", risks[idx])\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-12-10T06:45:23.874874Z","iopub.execute_input":"2025-12-10T06:45:23.875491Z","iopub.status.idle":"2025-12-10T06:45:25.032148Z","shell.execute_reply.started":"2025-12-10T06:45:23.875426Z","shell.execute_reply":"2025-12-10T06:45:25.030466Z"}},"outputs":[{"name":"stdout","text":"['ADANIPORTS', 'ASIANPAINT', 'AXISBANK', 'BAJAJ-AUTO', 'BAJAJFINSV']\n50\n","output_type":"stream"},{"name":"stderr","text":"/tmp/ipykernel_47/406060176.py:44: FutureWarning: The behavior of DataFrame concatenation with empty or all-NA entries is deprecated. In a future version, this will no longer exclude empty or all-NA columns when determining the result dtypes. To retain the old behavior, exclude the relevant entries before the concat operation.\n  df_all = pd.concat(parts, ignore_index=True)\n","output_type":"stream"},{"name":"stdout","text":"Symbol      ADANIPORTS  ASIANPAINT  AXISBANK  BAJAJ-AUTO  BAJAJFINSV  \\\nDate                                                                   \n2010-12-31   -0.106390    0.080979 -0.125809   -0.043065   -0.039631   \n2011-12-31   -0.172354   -0.098172 -0.409132    0.077819   -0.087479   \n2012-12-31    0.176983    0.711211  0.706782    0.444358    1.151053   \n2013-12-31    0.127721   -0.889217 -0.046342   -0.103687   -0.185323   \n2014-12-31    1.041267    0.505353 -0.611206    0.268619    0.720366   \n\nSymbol      BAJFINANCE  BHARTIARTL      BPCL  BRITANNIA     CIPLA  ...  \\\nDate                                                               ...   \n2010-12-31   -0.131680    0.100107 -0.134083  -0.000604  0.056722  ...   \n2011-12-31   -0.139908   -0.044107 -0.276006   0.095884 -0.141208  ...   \n2012-12-31    1.241051   -0.080336 -0.258377   0.117522  0.291504  ...   \n2013-12-31    0.158298    0.025303 -0.02